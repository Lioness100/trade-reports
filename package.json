  "main": "dist/index.js",
  "author": "Lioness100 <lioness.fiverr@gmail.com>",
  "type": "module",
  "private": true,
  "imports": {
    "#structures/*": "./dist/lib/structures/*.js",
    "#utils/*": "./dist/lib/utils/*.js",
    "#root/*": "./dist/*.js"
  },
  "scripts": {
    "pm2": "pm2 start ecosystem.yml",
    "start": "node --enable-source-maps .",
    "lint": "eslint .",
    "format": "prettier --write \"{*,src/**/*}.{json,js,ts,css,html}\"",
    "build": "tsup --config node_modules/@lioness100/configs/tsup.config.js --define.PROJECT_TYPE bot",
    "watch": "tsup --config node_modules/@lioness100/configs/tsup.config.js --define.PROJECT_TYPE bot --clean=false --watch"
  },
  "dependencies": {
    "@sapphire/decorators": "^6.2.0",
    "@sapphire/framework": "^5.3.7",
    "@sapphire/plugin-hmr": "^3.0.2",
    "@sapphire/string-store": "^2.0.0",
    "canvas": "2.10.2",
    "colorette": "^2.0.20",
    "discord.js": "^14.24.0",
    "dotenv": "^17.2.3",
    "envalid": "^8.1.0",
    "fuse.js": "^7.1.0",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.0.2",
    "grammy": "^1.39.2",
    "tslog": "^4.10.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5",
    "zlib-sync": "^0.1.10"
  },
  "devDependencies": {
    "@lioness100/configs": "1.0.34",
    "@types/node": "^24.9.1",
    "eslint": "^8.57.1",
    "pm2": "^6.0.13",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.11"
  },
  "prettier": "@lioness100/configs/prettier",
  "eslintConfig": {
    "extends": [
      "./node_modules/@lioness100/configs/.eslintrc.json"
    ]
  },
  "packageManager": "yarn@4.10.3"
}
